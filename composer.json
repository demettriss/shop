{
  "name": "jerex/shop",
  "description": "Laravel package example",
  "license": "MIT",
  "authors": [
    {
      "name": "Yuriy PLokhov",
      "email": "yurii.plohov@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/console": "^9.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.2",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jerex\\Shop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jerex\\Shop\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jerex\\Shop\\Providers\\EloquentFilterServiceProvider"
      ]
    }
  }
}
